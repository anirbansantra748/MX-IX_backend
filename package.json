{
  "name": "mx-ix-admin-backend",
  "version": "1.0.0",
  "description": "MX-IX Admin Panel Backend - Reusable template for content management",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --exec \"ts-node --transpile-only\" src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "start:prod": "NODE_ENV=production node dist/app.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "seed": "ts-node src/seed.ts"
  },
  "keywords": [
    "admin",
    "backend",
    "api",
    "express",
    "mongodb",
    "typescript"
  ],
  "author": "Wonder Creative Studio",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.3",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.9",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
